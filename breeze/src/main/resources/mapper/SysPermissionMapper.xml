<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.breeze.boot.system.mapper.SysPermissionMapper">

    <resultMap id="BaseResultMap" type="com.breeze.boot.system.domain.SysPermission">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="operator" column="operator" jdbcType="VARCHAR"/>
        <result property="sql" column="sql" jdbcType="VARCHAR"/>
        <result property="permissions" column="permissions" jdbcType="VARCHAR"/>
        <result property="createBy" column="create_by" jdbcType="VARCHAR"/>
        <result property="createName" column="create_name" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateBy" column="update_by" jdbcType="VARCHAR"/>
        <result property="updateName" column="update_name" jdbcType="VARCHAR"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
        <result property="deleteBy" column="delete_by" jdbcType="VARCHAR"/>
        <result property="tenantId" column="tenant_id" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,operator,`sql`,
        permissions,create_by,create_name,
        create_time,update_by,update_name,
        update_time,is_delete,delete_by,
        tenant_id
    </sql>
</mapper>
